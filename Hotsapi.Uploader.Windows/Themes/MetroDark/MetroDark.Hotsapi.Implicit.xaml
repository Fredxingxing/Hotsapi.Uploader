                    xmlns:local="clr-namespace:Hotsapi.Uploader.Windows">
    
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="MetroDark.MSControls.Core.Implicit.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource WindowStyle}" TargetType="{x:Type local:MainWindow}" />
